{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "tsc": {
      "dependsOn": ["^tsc"]
    },
    "check": {
      "dependsOn": ["^check"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "format": {
      "dependsOn": ["^format"]
    },
    "dev": {
      "inputs": [
        ".env*"
      ],
      "cache": false,
      "persistent": true
    },
    "dev:client": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": true
    },
    "dev:server": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": true
    },
    "docker": {
      "cache": false,
      "persistent": true
    },
    "docker:db": {
      "cache": false,
      "persistent": true
    },
    "generate": {
      "cache": false
    },
    "db:deploy": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    }
  }
}
